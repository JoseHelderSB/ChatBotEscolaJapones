[HeroCard
  title = KATAKANA
  text = O katakana é usado geralmente em palavras importadas de línguas estrangeiras. Pode ser usado para mostrar a pronúncia de kanjis em outras línguas como o Chinês, e assim como o itálico, também pode ser usado para dar ênfase à certas palavras.
  images = https://thumb.ac-illust.com/2d/2d4b269c206eb300f892ec4528e1b53d_t.jpeg
]

