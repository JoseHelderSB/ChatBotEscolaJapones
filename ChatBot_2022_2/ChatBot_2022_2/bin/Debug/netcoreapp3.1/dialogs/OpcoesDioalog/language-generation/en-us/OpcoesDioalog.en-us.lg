[HeroCard
  title = HIRAGANA
  text = Hiragana é um dos silabários da língua japonesa. É usado para todas as palavras para as quais não exista kanji, ou este exista mas seja pouco usado. Também é usado para substituir os kanji e nas terminações dos verbos e dos adjetivos.
  images = https://thumb.ac-illust.com/da/da4bc77768027221d2fa761b820cdc86_t.jpeg
]

